<div class='row filter-calendar'>

	<div class="input-daterange input-group" id="datepicker">
	    <input type="text" class="input-sm form-control" id="start" placeholder="Check In Date">
	    <span class="input-group-addon">to</span>
	    <input type="text" class="input-sm form-control" id="end" placeholder="Check Out Date">
	</div>

</div>

<script>
$('.input-daterange').datepicker({
    todayBtn: "linked",
	startDate: "today",
    beforeShowDay: function (date){
      if (date.getMonth() == (new Date()).getMonth())
        switch (date.getDate()){
          case 4:
            return {
              tooltip: 'Example tooltip',
              classes: 'active'
            };
          case 8:
            return false;
          case 12:
            return "green";
        }
    }
});

$('#start').on('changeDate', function(event) {
	$('#end').datepicker('setStartDate', event.date);
});

$('#end').on('changeDate', function(event) {
	$('#start').datepicker('setEndDate', event.date);
});
</script>


